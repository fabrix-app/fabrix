{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": "./",
    "outDir": "../dist",
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "strict": false,
    "noImplicitAny": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "es2017.object"
    ]
  },
  "include": [
    "../package.json",
    "test/**/*.ts",
    "**/*.ts",
    "index.ts"
  ],
  "exclude": [],
  "paths": {}
}
