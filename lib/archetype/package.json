{
  "version": "0.0.0",
  "keywords": [
    "fabrix"
  ],
  "scripts": {
    "build": "tsc -p ./src/tsconfig.release.json",
    "lint": "tslint -p ./src",
    "watch": "tsc -w -p ./src/tsconfig.release.json",
    "test": "npm run clean && npm run lint && npm run build && nyc mocha",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && npm run build",
    "clean": "rm -rf dist"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fabrix/fabrix": "^1",
    "@fabrix/spool-router": "^1"
  },
  "devDependencies": {
    "@fabrix/spool-repl": "^1",
    "@fabrix/lint": "^1.0.0-alpha.3",
    "@types/node": "~10.3.4",
    "mocha": "^5",
    "nyc": "^12.0.2",
    "supertest": "^3",
    "tslib": "~1.9.0",
    "tslint": "~5.10.0",
    "tslint-microsoft-contrib": "~5.0.3",
    "tsutils": "~2.27.1",
    "typescript": "~2.8.1"
  },
  "engines": {
    "node": ">= 7.6.0 =< 10.0.0",
    "npm": ">= 3.10.0"
  }
}
